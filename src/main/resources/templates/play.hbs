                        <div class="wooteco chess-col" data-x="6" data-y="0">
                        </div>
                        <div class="wooteco chess-col" data-x="7" data-y="0">
                        </div>
                    </div>
                </div>
                <div class="wooteco chess-rank">
                    <div class="wooteco chess-symbol">1</div>
                    <div class="wooteco chess-symbol">2</div>
                    <div class="wooteco chess-symbol">3</div>
                    <div class="wooteco chess-symbol">4</div>
                    <div class="wooteco chess-symbol">5</div>
                    <div class="wooteco chess-symbol">6</div>
                    <div class="wooteco chess-symbol">7</div>
                    <div class="wooteco chess-symbol">8</div>
                </div>
            </div>
            <div class="wooteco chess-file">
                <div class="wooteco chess-symbol">A</div>
                <div class="wooteco chess-symbol">B</div>
                <div class="wooteco chess-symbol">C</div>
                <div class="wooteco chess-symbol">D</div>
                <div class="wooteco chess-symbol">E</div>
                <div class="wooteco chess-symbol">F</div>
                <div class="wooteco chess-symbol">G</div>
                <div class="wooteco chess-symbol">H</div>
            </div>
        </div>
    </div>
</div>

<script src="/static/js/play.js">
</script>
</body>
</html>